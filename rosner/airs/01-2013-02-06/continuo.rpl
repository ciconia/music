\relative c {
  \key c \major
  \time 4/4
  
    \partial 2.
    [c8 g'] [c b] [a g]
  \repeat volta 2 { [f g] [a f] g4 g,
  c c' b c
  g c b c } \alternative {
    {g [c,8 g'] [c b] [a g]}
    {g4 e f g}
  }
  \repeat volta 2 { a e f g
    c, e f g
    a e f g
  } \alternative {
    {c, e f g}
    {c,\fermata}
  }
}
